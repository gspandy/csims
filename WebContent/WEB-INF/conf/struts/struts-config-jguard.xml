		<form-bean name="passwordResetForm"
			type="org.apache.struts.action.DynaActionForm">
			<form-property name="old" type="java.lang.String" />
			<form-property name="new" type="java.lang.String" />
			<form-property name="newAgain" type="java.lang.String" />
		</form-bean>
	</form-beans>

	<global-forwards>
		<forward name="logon" path="/Logon.do" contextRelative="true" />
		<forward name="logoff" path="/Logoff.do" />
		<forward name="vipArea" path="/VipArea.do" />
		<forward name="register" path="/Registration.do" />
	</global-forwards>

	<action-mappings>
		<action path="/AccessDenied"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="/WEB-INF/page/jguard/access_denied.jsp" />

		<action path="/AuthenticationFailed"
			parameter="/WEB-INF/page/jguard/authentication_failed.jsp"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" validate="false">
			<forward name="authenticationFailedOK" redirect="false"
				path="/WEB-INF/page/jguard/authentication_failed.jsp" />
		</action>

		<action path="/Logon" parameter="/WEB-INF/page/jguard/logon.jsp"
			type="org.springframework.web.struts.DelegatingActionProxy" />

		<action path="/LogonProcess"
			parameter="/WEB-INF/page/frame/welcome.jsp"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" />

		<action path="/Logoff"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="/WEB-INF/page/jguard/logonTemp.jsp" />

		<action path="/Registration"
			type="org.springframework.web.struts.DelegatingActionProxy"
			parameter="/WEB-INF/page/jguard/registration.jsp" name="generalForm" />

		<action path="/Captcha"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" />

		<action path="/User" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" scope="request">
			<forward name="toUserList" redirect="false"
				path="/WEB-INF/page/jguard/userList.jsp" />
			<forward name="toUserListOfNK" redirect="false"
				path="/WEB-INF/page/jguard/userListOfNK.jsp" />
			<forward name="toPluralUserList" redirect="false"
				path="/WEB-INF/page/jguard/pluralUserList.jsp" />
			<forward name="toAuditUserList" redirect="false"
				path="/WEB-INF/page/jguard/auditUserList.jsp" />
			<forward name="toAuditUserPage" redirect="false"
				path="/WEB-INF/page/jguard/auditUserPage.jsp" />
			<forward name="toApplyUserPage" redirect="false"
				path="/WEB-INF/page/jguard/applyUserPage.jsp" />
			<forward name="toApplyUserList" redirect="false"
				path="/WEB-INF/page/jguard/applyUserList.jsp" />
			<forward name="toCreateUserPage" redirect="false"
				path="/WEB-INF/page/jguard/createUserPage.jsp" />
			<forward name="toCreateUserPageOfNK" redirect="false"
				path="/WEB-INF/page/jguard/createUserPageOfNK.jsp" />
			<forward name="toUserInfoPage" redirect="false"
				path="/WEB-INF/page/jguard/userInfoPage.jsp" />
			<forward name="toAddUserInfoPage" redirect="false"
				path="/WEB-INF/page/jguard/addUserInfoPage.jsp" />
			<forward name="toUpdateUserPage" redirect="false"
				path="/WEB-INF/page/jguard/updateUserPage.jsp" />
			<forward name="toUpdateUserPageOfNK" redirect="false"
				path="/WEB-INF/page/jguard/updateUserPageOfNK.jsp" />
			<forward name="toUserMessgePage" redirect="false"
				path="/WEB-INF/page/jguard/userMessgePage.jsp" />
			<forward name="createUserOK" redirect="false"
				path="/User.do?method=list" />
			<forward name="deleteUserOK" redirect="false"
				path="/User.do?method=list" />
			<forward name="updateUserOK" path="/User.do?method=list"
				redirect="false" />
			<forward name="toPersonalInfoPage" redirect="false"
				path="/WEB-INF/page/jguard/personalInfoPage.jsp" />
			<forward name="toUserReportOfPList" redirect="false"
				path="/WEB-INF/page/jguard/userReportOfPList.jsp" />
			<forward name="toUserReportInfoPageOfP" redirect="false"
				path="/WEB-INF/page/jguard/userReportInfoPageOfP.jsp" />
			<forward name="toUserReportOfTList" redirect="false"
				path="/WEB-INF/page/jguard/userReportOfTList.jsp" />
			<forward name="toUserStopExcelPage" redirect="false"
				path="/WEB-INF/page/jguard/userStopExcelPage.jsp" />
			<forward name="toUserStatisticsPage" redirect="false"
				path="/WEB-INF/page/jguard/userStatisticsPage.jsp" />
			<forward name="toUserStatisticsResultPage" redirect="false"
				path="/WEB-INF/page/jguard/userStatisticsResultPage.jsp" />
			<forward name="toUserInfoQueryPage" redirect="false"
				path="/WEB-INF/page/jguard/userInfoQueryPage.jsp" />
			<forward name="toUserInfoQueryResultPage" redirect="false"
				path="/WEB-INF/page/jguard/userInfoQueryResultPage.jsp" />
			<forward name="toUserInfoQueryResultPageOfJg" redirect="false"
				path="/WEB-INF/page/jguard/userInfoQueryResultPageOfJg.jsp" />
			<forward name="toUploadUserComInfoPage" redirect="false"
				path="/WEB-INF/page/jguard/uploadUserComInfoPage.jsp" />
		</action>

		<action path="/Principal" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" scope="request">
			<forward name="listPrincipalsOK" redirect="false"
				path="/WEB-INF/page/jguard/principalsList.jsp" />
			<forward name="createPrincipalOK" redirect="false"
				path="/Principal.do?method=list" />
			<forward name="addInheritanceOK" redirect="false"
				path="/Principal.do?method=list" />
			<forward name="deletePrincipalOK" redirect="false"
				path="/Principal.do?method=list" />
			<forward name="updatePrincipalOK"
				path="/Principal.do?method=list" redirect="false" />
			<forward name="clonePrincipalOK"
				path="/Principal.do?method=list" redirect="false" />
			<forward name="readPrincipalOK"
				path="/WEB-INF/page/jguard/principal.jsp" redirect="false" />
			<forward name="readPrincipalCheckBoxOK"
				path="/WEB-INF/page/jguard/principalCheckBox.jsp" redirect="false" />
		</action>

		<action path="/Permission"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm" parameter="method">
			<forward name="createPermissionOK" redirect="false"
				path="/Domain.do?method=list" />
			<forward name="listPermissionsOK"
				path="/WEB-INF/page/jguard/permissionsList.jsp" redirect="false"
				contextRelative="true" />
			<forward name="updatePermissionOK" redirect="false"
				path="/Domain.do?method=list" />
			<forward name="deletePermissionOK" redirect="false"
				path="/Domain.do?method=list" />
		</action>

		<action path="/Domain" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="generalForm">
			<forward name="listDomainsOK"
				path="/WEB-INF/page/jguard/permissionsList.jsp" redirect="false" />
			<forward name="createDomainOK" redirect="false"
				contextRelative="false" path="/Domain.do?method=list" />
			<forward name="deleteDomainOK" redirect="false"
				path="/Domain.do?method=list" />
			<forward name="updateDomainOK" path="/Domain.do?method=list"
				redirect="false" />
		</action>

		<action path="/resetPassword" parameter="method"
			type="org.springframework.web.struts.DelegatingActionProxy"
			name="passwordResetForm">
			<forward name="toReset"
				path="/WEB-INF/page/jguard/passwordReset.jsp" redirect="false" />
		</action>
	</action-mappings>
</struts-config>
