<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gtm.csims.model">
	<class
		name="BsAepeople"
		table="BS_AEPEOPLE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>

		<property
			name="Pepname"
			column="PEPNAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Cardid"
			column="CARDID"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Certtp"
			column="CERTTP"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Certno"
			column="CERTNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Bankno"
			column="BANKNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Creditno"
			column="CREDITNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Orgno"
			column="ORGNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Orgnm"
			column="ORGNM"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Dptnm"
			column="DPTNM"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Dptno"
			column="DPTNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Prcipal"
			column="PRCIPAL"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Tele"
			column="TELE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Stat"
			column="STAT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Updateate"
			column="UPDATEATE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Sex"
			column="SEX"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="Education"
			column="EDUCATION"
			type="string"
			not-null="false"
			length="20"
		/>
	</class>	
</hibernate-mapping>