<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gtm.csims.model">
	<class
		name="BsAttcachments"
		table="BS_ATTCACHMENTS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<!-- <generator class="org.hibernate.id.UUIDHexGenerator"/>  -->
		</id>

		<property
			name="Wfid"
			column="WFID"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Attchname"
			column="ATTCHNAME"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Attchuploader"
			column="ATTCHUPLOADER"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Attchpath"
			column="ATTCHPATH"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Attchofflow"
			column="ATTCHOFFLOW"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="Updatedate"
			column="UPDATEDATE"
			type="date"
			not-null="false"
			length="10"
		/>
	</class>	
</hibernate-mapping>