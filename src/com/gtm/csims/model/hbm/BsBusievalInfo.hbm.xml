<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gtm.csims.model">
	<class
		name="BsBusievalInfo"
		table="BS_BUSIEVALINFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
			<generator class="org.hibernate.id.UUIDHexGenerator"/>
		</id>
		<property
			name="EvalCondition"
			column="EVALCONDITION"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="EvalInfo"
			column="EVALINFO"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="EvalContent"
			column="EVALCONTENT"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="EvalRule"
			column="EVALRULE"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Evaldt"
			column="EVALDT"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="DeScore"
			column="DESCORE"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="BaseScore"
			column="BASESCORE"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="Histrec"
			column="HISTREC"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Creator"
			column="CREATOR"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Crtdate"
			column="CRTDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Stat"
			column="STAT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Updatedate"
			column="UPDATEDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<many-to-one
			name="BsBusieval"
			column="BSBUSIEVAL"
			class="BsBusieval"
			not-null="false"
		>
		</many-to-one>
		
	</class>	
</hibernate-mapping>