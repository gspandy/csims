		<property
			name="Qcrtdate"
			column="QCRTDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Qisenable"
			column="QISENABLE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Qisfinished"
			column="QISFINISHED"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Qstat"
			column="QSTAT"
			type="java.lang.Short"
			not-null="false"
			length="5"
		/>
		<property
			name="Qenddatetime"
			column="QENDDATETIME"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Stat"
			column="STAT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Updateate"
			column="UPDATEATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>


