<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.gtm.csims.model">
	<class
		name="BsQuestionaire"
		table="BS_QUESTIONAIRE"
	>
		<meta attribute="sync-DAO">false</meta>

		 <id
			name="Id"
			type="string"
			column="ID"
		>
			<!-- <generator class="org.hibernate.id.UUIDHexGenerator"/> -->
		</id>

		<property
			name="Qtitle"
			column="QTITLE"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="Qsumry"
			column="QSUMRY"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="Qcreator"
			column="QCREATOR"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Qcreatororgname"
			column="QCREATORORGNAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Qcreatororgno"
			column="QCREATORORGNO"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Qenddatetime"
			column="QENDDATETIME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="STATUS"
			type="string"
			not-null="false"
			length="5"
		/>
		<property
			name="Flag"
			column="FLAG"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Createdate"
			column="CREATEDATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>
		<property
			name="Updateate"
			column="UPDATEATE"
			type="timestamp"
			not-null="false"
			length="26"
		/>


	</class>	
</hibernate-mapping>